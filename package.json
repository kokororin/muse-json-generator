  },
  "homepage": "https://github.com/moefront/muse-json-generator#readme",
  "dependencies": {
    "chalk": "^2.2.0",
    "commander": "^2.11.0",
    "honoka": "^0.4.4",
    "lodash": "^4.17.15",
    "ora": "^1.3.0",
    "pjson": "^1.0.9",
    "pretty-error": "^2.1.1",
    "semver": "^5.4.1"
  },
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/chai-as-promised": "^7.1.0",
