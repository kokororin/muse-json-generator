    "coveralls": "^3.0.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.5.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nock": "^9.0.22",
    "trash-cli": "^1.4.0",
    "ts-node": "^3.3.0",
