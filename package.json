{
  "name": "muse-json-generator",
  "version": "2.0.0",
  "description": "JSON generator for MUSE",
  "main": "lib/generator.js",
  "bin": {
    "muse": "bin/muse.js",
    "muse-json-generator": "bin/muse.js"
  },
  "scripts": {
    "start": "npm run watch",
    "build": "npm run trash && tsc -p ./",
    "pretest": "npm run build",
    "test": "istanbul cover _mocha -- --compilers ts:ts-node/register --timeout 30000 test/**/*.ts",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "trash": "trash lib",
    "watch": "tsc -watch -p ./",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moefront/muse-json-generator.git"
  },
  "keywords": [
    "muse",
    "ymplayer"
  ],
  "author": "MoeFront Studio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moefront/muse-json-generator/issues"
  },
  "homepage": "https://github.com/moefront/muse-json-generator#readme",
  "dependencies": {
    "honoka": "^0.4.4",
    "ora": "^1.3.0",
    "pretty-error": "^2.1.1",
    "yargs": "^9.0.1"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.44",
    "@types/power-assert": "^1.4.29",
    "coveralls": "^3.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0",
    "mocha-lcov-reporter": "^1.3.0",
    "power-assert": "^1.4.4",
    "trash-cli": "^1.4.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.3"
  },
  "files": [
    "lib",
    "bin"
  ]
}
