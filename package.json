{
  "name": "muse-json-generator",
  "version": "1.0.7",
  "description": "JSON generator for MUSE",
  "main": "lib/generator.js",
  "bin": {
    "muse": "bin/muse.js",
    "muse-json-generator": "bin/muse.js"
  },
  "scripts": {
    "build": "npm run trash && babel src --out-dir lib",
    "test": "mocha --compilers js:babel-register --require babel-polyfill --timeout 30000 test/**/*.js && npm run lint",
    "trash": "trash build stats lib",
    "watch": "babel src --watch --out-dir lib",
    "lint": "eslint ./src ./bin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moefront/muse-json-generator.git"
  },
  "keywords": [
    "muse",
    "ymplayer"
  ],
  "author": "MoeFront Studio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moefront/muse-json-generator/issues"
  },
  "homepage": "https://github.com/moefront/muse-json-generator#readme",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "honoka": "^0.4.1",
    "pretty-error": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.5.0",
    "eslint-config-kotori": "^0.1.3",
    "mocha": "^3.5.0",
    "power-assert": "^1.4.4",
    "trash-cli": "^1.4.0"
  },
  "files": [
    "lib",
    "bin"
  ]
}
